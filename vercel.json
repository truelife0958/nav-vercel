{
  "version": 2,
  "name": "nav-pro-vercel",
  "builds": [
    {
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
    "distDir": "frontend/dist"
    }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
  { "src": "/api/(.*)", "dest": "/api/index.js" },
  { "src": "/(.*)", "dest": "/index.html" }
  ]
  }
